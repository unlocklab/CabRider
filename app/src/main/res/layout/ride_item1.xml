<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/margin_md"
    android:elevation="2dp"
    android:outlineAmbientShadowColor="@color/app_color"
    app:cardCornerRadius="@dimen/margin_lg"
    app:cardBackgroundColor="@color/white">

    <RelativeLayout
        android:id="@+id/rl1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/margin1">
        <TextView
            android:id="@+id/txt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/font1"
            android:gravity="right"
            android:layout_marginBottom="@dimen/margin_lg"
            android:includeFontPadding="false"
            android:text="20 May 2020"
            android:textColor="@color/dgry1"
            android:textSize="@dimen/txt_size_md1" />
        <RelativeLayout
            android:id="@+id/right_rl"
            android:layout_below="@id/txt3"
            android:layout_width="@dimen/size9"
            android:layout_alignParentRight="true"
            android:layout_height="wrap_content">


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:paddingTop="@dimen/margin1"
                android:layout_marginTop="@dimen/size6"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/accept_bt"
                    android:layout_width="@dimen/size06"
                    android:layout_height="@dimen/size3"
                    android:background="@drawable/round_ract0"
                    >

                    <TextView
                        android:id="@+id/track_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:fontFamily="@font/font2"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:singleLine="true"
                        android:text="@string/track"
                        android:textColor="@color/app_color"
                        android:textSize="@dimen/txt_size_md" />
                </RelativeLayout>

            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/left_rl"
            android:layout_below="@id/txt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/m1"
                android:layout_width="@dimen/size2"
                android:src="@drawable/loc_icon2"
                android:layout_centerHorizontal="true"
                android:tint="@color/black"
                android:layout_height="@dimen/size2"/>
            <ImageView
                android:id="@+id/m2"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m1"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m3"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m2"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m4"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m3"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m5"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m4"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m6"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m5"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m7"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m6"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m8"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m7"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:id="@+id/m9"
                android:layout_width="@dimen/sizesm1"
                android:layout_below="@id/m8"
                android:layout_marginTop="@dimen/sizesm"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle_shape"
                android:backgroundTint="@color/dgry"
                android:layout_height="@dimen/sizesm1"/>
            <ImageView
                android:layout_below="@id/m9"
                android:layout_width="@dimen/size2"
                android:src="@drawable/loc_icon2"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/margin"
                android:tint="@color/app_color"
                android:layout_height="@dimen/size2"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@id/txt3"
            android:layout_width="wrap_content"
            android:layout_toLeftOf="@id/right_rl"
            android:layout_marginLeft="@dimen/sizesm1"
            android:layout_toRightOf="@id/left_rl"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/txt1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/font2"
                android:includeFontPadding="false"
                android:text="Location 1"
                android:textColor="@color/black"
                android:textSize="@dimen/txt_size_md1" />

            <TextView
                android:id="@+id/txt2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt1"
                android:paddingTop="@dimen/margin1"
                android:layout_marginTop="@dimen/size6"
                android:fontFamily="@font/font2"
                android:includeFontPadding="false"
                android:text="Location 1"
                android:textColor="@color/black"
                android:textSize="@dimen/txt_size_md1" />
        </RelativeLayout>
    </RelativeLayout>
</androidx.cardview.widget.CardView>